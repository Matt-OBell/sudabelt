<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <record id="ir_cron_scheduler_hr_holidays" model="ir.cron">
        <field name="name">Email notification for expired products within six months</field>
        <field name="model_id" ref="model_stock_production_lot"/>
        <field name="state">code</field>
        <field name="code">model._cron_notify_scheduler_queue(days=183)</field>
        <field name="user_id" ref="base.user_root"/>
